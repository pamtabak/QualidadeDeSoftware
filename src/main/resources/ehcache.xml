<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.mycompany.myapp.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="com.mycompany.myapp.domain.Programa"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Programa.programas"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Usuario"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Aluno"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Aluno.alunos"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Professor"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Professor.orientadors"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Professor.professors"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.SecretariaAcademica"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Disciplina"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Disciplina.disciplinas"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.mycompany.myapp.domain.Inscricao"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
